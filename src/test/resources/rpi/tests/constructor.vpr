field n: Ref

method main()
{
    var x: Ref
    var i: Int

    x := null
    i := 10

    while (i > 0)
    {
        var t: Ref
        t := new(n)
        t.n := x
        x := t
        i := i - 1
        __foldUp__(x)
    }

    while (x != null)
    {
        __unfoldDown__(x)
        x := x.n
    }
}