field n: Ref

method main()
{
    var c: Ref
    var i: Int

    c := null
    i := 10

    __up__(c)
    while (i > 0)
    {
        var t: Ref
        t := new(n)
        t.n := c
        c := t
        i := i - 1
        __up__(c)
    }

    while (c != null)
    {
        c := c.n
        __down__(c)
    }
}